#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/aphilack/Downloads/dotdeep-design-final/portfolio-website/node_modules/.pnpm/vite@6.3.5_@types+node@24.0.13_jiti@2.4.2_lightningcss@1.30.1/node_modules/vite/bin/node_modules:/Users/aphilack/Downloads/dotdeep-design-final/portfolio-website/node_modules/.pnpm/vite@6.3.5_@types+node@24.0.13_jiti@2.4.2_lightningcss@1.30.1/node_modules/vite/node_modules:/Users/aphilack/Downloads/dotdeep-design-final/portfolio-website/node_modules/.pnpm/vite@6.3.5_@types+node@24.0.13_jiti@2.4.2_lightningcss@1.30.1/node_modules:/Users/aphilack/Downloads/dotdeep-design-final/portfolio-website/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/aphilack/Downloads/dotdeep-design-final/portfolio-website/node_modules/.pnpm/vite@6.3.5_@types+node@24.0.13_jiti@2.4.2_lightningcss@1.30.1/node_modules/vite/bin/node_modules:/Users/aphilack/Downloads/dotdeep-design-final/portfolio-website/node_modules/.pnpm/vite@6.3.5_@types+node@24.0.13_jiti@2.4.2_lightningcss@1.30.1/node_modules/vite/node_modules:/Users/aphilack/Downloads/dotdeep-design-final/portfolio-website/node_modules/.pnpm/vite@6.3.5_@types+node@24.0.13_jiti@2.4.2_lightningcss@1.30.1/node_modules:/Users/aphilack/Downloads/dotdeep-design-final/portfolio-website/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@6.3.5_@types+node@24.0.13_jiti@2.4.2_lightningcss@1.30.1/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@6.3.5_@types+node@24.0.13_jiti@2.4.2_lightningcss@1.30.1/node_modules/vite/bin/vite.js" "$@"
fi
